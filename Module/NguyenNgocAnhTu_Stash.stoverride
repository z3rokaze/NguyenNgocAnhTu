    interval: 86400
  pa-res:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/PicsArt.js
    interval: 86400
  sc-res:
    url: https://raw.githubusercontent.com/sbunkov/shadowrocket-configuration/main/scripts/soundcloud.js
    interval: 86400
  am-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/alight-motion.json
    interval: 86400
  km-res:

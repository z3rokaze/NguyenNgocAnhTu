    interval: 86400
  pm-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/Photomath.json
    interval: 86400
  lr-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/Lightroom.json
    interval: 86400
  dj-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/djay.json
    interval: 86400
  calm-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/calm.json
    interval: 86400
  bussu-res:
    url: https://raw.githubusercontent.com/iSteal-it/script/main/bussu.json
    interval: 86400
